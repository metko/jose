{
    "name": "metko/jose2",
    "type": "library",
    "description": "Common Container Interface (PHP FIG PSR-11)",
    "keywords": ["psr", "psr-11", "container", "container-interop", "container-interface"],
    "homepage": "https://github.com/php-fig/container",
    "license": "MIT",
    "authors": [
        {
            "name": "PHP-FIG",
            "homepage": "https://www.php-fig.org/"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "filp/whoops": "^2.11",
        "symfony/var-dumper": "^5.2",
        "symfony/filesystem": "5.2",
        "symfony/finder": "5.2",
        "stoutlogic/acf-builder": "1.4.0"
    },

    "autoload": {
        "files": [ "src/functions.php", "src/constants.php", "src/helpers.php" ],
        "psr-4": {
            "Jose\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "scripts": {
        "test": [
            "./vendor/bin/phpunit"
        ]
    }
}
