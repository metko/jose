{
    "name": "twn/jose",
    "description": "A simple bundler for bedrock. Documentation incoming",
    "keywords": [
        "wordpress",
        "jose"
    ],
    "homepage": "https://gitlab.walkingnerds.dev/twn/jose",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Thomas Moiluaivon",
            "email": "thomas.m@thewalkingnerds.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "filp/whoops": "^2.7",
        "symfony/filesystem": "^5.1",
        "symfony/finder": "^5.1",
        "symfony/var-dumper": "^5.1",
        "timber/timber": "^1.18",
        "composer/installers": "^1.0@dev",
        "stoutlogic/acf-builder": "1.11.0",
        "palmiak/timber-acf-wp-blocks" : "1.14"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Jose\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jose\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
